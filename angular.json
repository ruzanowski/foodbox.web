{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Food": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/assets/img/loader.gif",
              "src/assets/video/intro.mp4",
              "src/assets/img/boxed-food-table.jpg",
              "src/assets/img/thumb_restaurant.jpg",
              "src/assets/img/thumb_restaurant_2.jpg",
              "src/assets/img/thumb_restaurant_3.jpg",
              "src/assets/img/thumb_restaurant_4.jpg",
              "src/assets/img/thumb_restaurant_5.jpg",
              "src/assets/img/thumb_restaurant_6.jpg",
              {
                "glob": "abp.signalr.js",
                "input": "node_modules/abp-web-resources/Abp/Framework/scripts/libs",
                "output": "/assets/abp"
              },
              {
                "glob": "abp.signalr-client.js",
                "input": "node_modules/abp-web-resources/Abp/Framework/scripts/libs",
                "output": "/assets/abp"
              }
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/famfamfam-flags/dist/sprite/famfamfam-flags.css",
              "node_modules/sweetalert2/dist/sweetalert2.css",
              "src/assets/freeze-ui/freeze-ui.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "node_modules/admin-lte-css-only/css/adminlte.min.css",
              "src/shared/core.less",
              "src/assets/css/animate.min.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/bootstrap.css",
              "src/assets/css/menu.css",
              "src/assets/css/style.css",
              "src/assets/css/responsive.css",
              "src/assets/css/elegant_font/elegant_font.min.css",
              "src/assets/css/fontello/css/fontello.min.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/pop_up.css",
              "src/assets/css/app-loading.scss"
            ],
            "scripts": [
              "node_modules/moment/min/moment.min.js",
              "node_modules/@aspnet/signalr/dist/browser/signalr.min.js",
              "node_modules/sweetalert2/dist/sweetalert2.js",
              "src/assets/freeze-ui/freeze-ui.js",
              "node_modules/push.js/bin/push.min.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/abp.js",
              "src/assets/abp-web-resources/abp.sweet-alert.js",
              "src/assets/abp-web-resources/abp.freeze-ui.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/libs/abp.moment.js",
              "src/assets/js/bootstrap-datepicker.js",
              "src/assets/js/bootstrap-timepicker.js",
              "src/assets/js/bootstrap.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/bootstrap3-wysihtml5.min.js",
              "src/assets/js/cat_nav_mobile.js",
              "src/assets/js/common_scripts_min.js",
              "src/assets/js/dropzone.js",
              "src/assets/js/dropzone.min.js",
              "src/assets/js/functions.js",
              "src/assets/js/html5shiv.min.js",
              "src/assets/js/icheck.js",
              "src/assets/js/ion.rangeSlider.js",
              "src/assets/js/jquery-1.11.2.min.js",
              "src/assets/js/jquery-2.2.4.min.js",
              "src/assets/js/jquery.cookiebar.js",
              "src/assets/js/jquery.magnific-popup.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/jquery.placeholder.js",
              "src/assets/js/jquery.placeholder.min.js",
              "src/assets/js/jquery.sliderPro.js",
              "src/assets/js/jquery.placeholder.min.js",
              "src/assets/js/map.js",
              "src/assets/js/map_single.js",
              "src/assets/js/modernizr.js",
              "src/assets/js/morphext.js",
              "src/assets/js/morphext.min.js",
              "src/assets/js/parallax.js",
              "src/assets/js/parallax.min.js",
              "src/assets/js/pw_strenght.js",
              "src/assets/js/ResizeSensor.js",
              "src/assets/js/ResizeSensor.min.js",
              "src/assets/js/respond.min.js",
              "src/assets/js/retina-replace.js",
              "src/assets/js/retina-replace.min.js",
              "src/assets/js/tabs.js",
              "src/assets/js/theia-sticky-sidebar.js",
              "src/assets/js/theia-sticky-sidebar.min.js",
              "src/assets/js/video_header.js",
              "src/assets/js/wow.js",
              "src/assets/js/wow.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "hmr": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.hmr.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "Food:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "Food:build:production"
            },
            "hmr": {
              "browserTarget": "Food:build:hmr"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Food:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.json",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              {
                "glob": "abp.signalr.js",
                "input": "node_modules/abp-web-resources/Abp/Framework/scripts/libs",
                "output": "/assets/abp"
              },
              {
                "glob": "abp.signalr-client.js",
                "input": "node_modules/abp-web-resources/Abp/Framework/scripts/libs",
                "output": "/assets/abp"
              }
            ],
            "styles": [
              "node_modules/famfamfam-flags/dist/sprite/famfamfam-flags.css",
              "node_modules/sweetalert2/dist/sweetalert2.css",
              "src/assets/freeze-ui/freeze-ui.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "node_modules/admin-lte-css-only/css/adminlte.min.css",
              "src/shared/core.less"
            ],
            "scripts": [
              "node_modules/moment/min/moment.min.js",
              "node_modules/@aspnet/signalr/dist/browser/signalr.min.js",
              "node_modules/sweetalert2/dist/sweetalert2.js",
              "src/assets/freeze-ui/freeze-ui.js",
              "node_modules/push.js/bin/push.min.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/abp.js",
              "src/assets/abp-web-resources/abp.sweet-alert.js",
              "src/assets/abp-web-resources/abp.freeze-ui.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/libs/abp.moment.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["src/tsconfig.json"],
            "exclude": []
          }
        }
      }
    },
    "Food-e2e": {
      "root": "e2e",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "Food:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["e2e/tsconfig.json"],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "Food",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "",
      "style": "css"
    },
    "@schematics/angular:directive": {
      "prefix": ""
    }
  },
  "cli": {
    "analytics": false
  }
}
