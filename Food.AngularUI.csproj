<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>angular</AssemblyName>
    <PackageId>angular</PackageId>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="node_modules\**" />
    <EmbeddedResource Remove="node_modules\**" />
    <None Remove="node_modules\**" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="dist" />
    <None Include="app.config" />
    <None Update="wwwroot\**\*;web.config">
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <None Update="Dockerfile">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <Content Include="src\assets\.DS_Store" />
    <Content Include="src\assets\assets\.DS_Store" />
    <Content Include="src\assets\coming_soon\.DS_Store" />
    <Content Include="src\assets\css\.DS_Store" />
    <Content Include="src\assets\img\.DS_Store" />
    <Content Include="src\assets\js\.DS_Store" />
    <Content Include="src\assets\layerslider\.DS_Store" />
    <Content Include="src\assets\video\.DS_Store" />
  </ItemGroup>
</Project>